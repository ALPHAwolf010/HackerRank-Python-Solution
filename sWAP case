def swap_case(s):
    l1=[];
    for x in s:
        if x.isalpha():
         if x.islower():
            l1.append(x.upper());
         else:
            l1.append(x.lower());    
        else:
            l1.append(x);
    # print(l1);
    l2=''.join(l1);
    # print(l2);
    return l2;
    

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
